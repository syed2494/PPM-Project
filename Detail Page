here iam Creating Detail Page using Lightning Component which it will fetch the user detail from the salesforce Org. From this we can Edit Or save the Data .

Component---------------------------------->


<aura:component implements="force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,forceCommunity:availableForAllPageTypes,force:hasrecordId,force:lightningQuickAction" access="global" controller="UpdateModel"> 

<aura:handler name="init" value="{!this}" action="{!c.doInit}"/>
     <aura:handler event="force:refreshView" action="{!c.init}"/>
   
    <aura:handler name="init" value="{!this}" action="{!c.SessionTimeOut}"/>
    <aura:attribute name="firstName" type="String"/>
    <aura:attribute name="lastName" type="String"/>
    <aura:attribute name="ProfileCompletion" type="String"/>    
    <aura:attribute name="selectedId" type="Id"/>
    <aura:attribute name="HelloId" type="Id"/>
      <aura:attribute name="EmergencyId" type="Id"/>
     <aura:attribute name="AddressId" type="Id"/>    
       <aura:attribute name="WorkExperienceId" type="Id"/>
    <aura:attribute name="CompanyProjectId" type="Id"/>
    <aura:attribute name="TechnicalSkillId" type="Id"/>
      <aura:attribute name="EducationalInfoId" type="Id"/>
      <aura:attribute name="CertificationId" type="Id"/>
      <aura:attribute name="TrainingAwardId" type="Id"/>
     <aura:attribute name="PassportId" type="Id"/>
      <aura:attribute name="VisaId" type="Id"/>
    
    
    
    
    
     <aura:handler name="init" value="{!this}" action="{!c.Email}"/>   
      <aura:attribute name="EmailsList" type="list" default="Email__c[]"/>    
      <aura:attribute name="emailobj" type="Email__c" default="{'sobjectType':'Email__c'}"/>
    
       <aura:handler name="init" value="{!this}" action="{!c.Phone}"/>    
      <aura:attribute name="PhonesList" type="list" default="Phone__c[]"/>    
      <aura:attribute name="phoneobj" type="Phone__c" default="{'sobjectType':'Phone__c'}"/>
            
         <aura:handler name="init" value="{!this}" action="{!c.EmergencyContact}"/>    
      <aura:attribute name="EmergencyContactsList" type="list" default="Emergency_Contact__c[]"/>    
      <aura:attribute name="emergencyContactobj" type="Emergency_Contact__c" default="{'sobjectType':'Emergency_Contact__c'}"/>
        
     <aura:handler name="init" value="{!this}" action="{!c.Address}"/>    
      <aura:attribute name="AddressList" type="list" default="Address__c[]"/>    
      <aura:attribute name="addressobj" type="Address__c" default="{'sobjectType':'Address__c'}"/>
       
       <aura:handler name="init" value="{!this}" action="{!c.WorkExperience}"/>    
      <aura:attribute name="WorkExperienceList" type="list" default="Work_Experience__c[]"/>    
      <aura:attribute name="WorkExperienceobj" type="Work_Experience__c" default="{'sobjectType':'Work_Experience__c'}"/>
    
     <aura:handler name="init" value="{!this}" action="{!c.CompanyProject}"/>    
      <aura:attribute name="CompanyProjectList" type="list" default="Company_Project__c[]"/>    
