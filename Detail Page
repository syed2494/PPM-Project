     <aura:handler name="init" value="{!this}" action="{!c.TechnicalSkill}"/>    
      <aura:attribute name="TechnicalSkillList" type="list" default="Technical_Skill__c[]"/>    
      <aura:attribute name="technicalSkillobj" type="Technical_Skill__c" default="{'sobjectType':'Technical_Skill__c'}"/>
    
      <aura:handler name="init" value="{!this}" action="{!c.EducationalInfo}"/>    
      <aura:attribute name="EducationalInfoList" type="list" default="Educational_Info__c[]"/>    
      <aura:attribute name="educationalInfoobj" type="Educational_Info__c" default="{'sobjectType':'Educational_Info__c'}"/>

    
      <aura:handler name="init" value="{!this}" action="{!c.Certification}"/>    
